	//start Conditionals
	enum ConditionalType //start
	{
		COND_NONE,
		COND_SCRIPT,
		COND_LITEM,
		COND_ITEM,
		COND_COUNTER,
		COND_COUNTER_UNDER,
		MAX_COND_TYPE
	}; //end
	//start misc enums for conditions
	enum //LItem indexes
	{
		LICND_TRIFORCE,
		LICND_MAP,
		LICND_COMPASS,
		LICND_BOSS_DEAD,
		LICND_BOSSKEY,
		MAX_LICND
	};
	//end
	bool getConditional(ConditionalType t, int indx, int indx2) //start
	{
		#option BINARY_32BIT off
		switch(t)
		{
			case COND_NONE: default:
				return true;
			case COND_LITEM:
				if(indx == -1)
					return Game->LItems[Game->GetCurLevel()] & (1b<<indx2);
				return Game->LItems[indx] & (1b<<indx2);
			case COND_SCRIPT:
				return get_32b(script_conditional, indx, indx2);
			case COND_ITEM:
				return Hero->Item[indx];
			case COND_COUNTER:
				unless(indx2) //Hardcode to check for a full counter
					return Game->Counter[indx] >= Game->MCounter[indx];
				return Game->Counter[indx] >= indx2;
			case COND_COUNTER_UNDER:
				return Game->Counter[indx] <= indx2;
		}
	} //end
	bool get_32b(untyped arr, int indx, int indx2) //start
	{
		#option BINARY_32BIT on
		return arr[indx] & 1b<<indx2;
	} //end
	void set_32b(untyped arr, int indx, int indx2, bool state) //start
	{
		#option BINARY_32BIT on
		if(state)
			arr[indx] |= 1b<<indx2;
		else
			arr[indx] ~= 1b<<indx2;
	} //end
	//These functions are for user scripts:
	bool getScriptConditional(int indx, int bit) //start
	{
		if(bit > 31 || bit < 0 || indx < 0) return false;
		return get_32b(script_conditional, indx, bit);
	} //end
	void setScriptConditional(int indx, int bit, bool state) //start
	{
		if(bit > 31 || bit < 0 || indx < 0) return;
		set_32b(script_conditional, indx, bit, state);
	} //end
