	//start Minimap
	void minimap(untyped module_arr, int offs, bitmap bit, bool active)
	{
		minimap(module_arr, offs, bit, active, module_arr[offs+M_X], module_arr[offs+M_Y]);
	}
	void minimap(untyped module_arr, int offs, bitmap bit, bool active, int orig_x, int orig_y)
	{
		dmapdata dm = Game->LoadDMapData(Game->GetCurDMap());
		int curscr = Game->GetCurDMapScreen();
		bool showExplore;
		int ow = dm->Type & (11b);
		int mtile = (ow == DMAP_OVERWORLD ? module_arr[offs+P7] : module_arr[offs+P9]);
		int mcs = (ow == DMAP_OVERWORLD ? module_arr[offs+P8] : module_arr[offs+P10]);
		bool hasMap = Game->LItems[Game->GetCurLevel()] & LI_MAP;
		unless(module_arr[offs+M_FLAGS1] & FLAG_MMP_IGNORE_DMAP_BGTILE)
		{
			if(dm->MiniMapTile[hasMap ? 1 : 0])
			{
				mtile = dm->MiniMapTile[hasMap ? 1 : 0];
				mcs = dm->MiniMapCSet[hasMap ? 1 : 0];
				//printf("without (%d,%d) - with (%d,%d)\n",dm->MiniMapTile[0],dm->MiniMapCSet[0],dm->MiniMapTile[1],dm->MiniMapCSet[1]);
			}
		}
		if(mtile) bit->DrawTile(module_arr[offs+M_LAYER], orig_x, orig_y, mtile, 5, 3, mcs, -1, -1, 0, 0, 0, 0, true, OP_OPAQUE);
		bool hasCompass = Game->LItems[Game->GetCurLevel()] & LI_COMPASS;
		bool deadCompass = Game->LItems[Game->GetCurLevel()] & ((module_arr[offs+M_FLAGS1] & FLAG_MMP_COMP_ON_BOSS) ? LI_BOSS : LI_TRIFORCE);
		bool blinkon = g_arr[active ? ACTIVE_TIMER : PASSIVE_TIMER] & 1 << (module_arr[offs+P6]-1);
		switch(ow)
		{
			case DMAP_OVERWORLD:
				showExplore = (module_arr[offs+M_FLAGS1] & FLAG_MMP_SHOW_EXPLORED_ROOMS_OW);
				break;
			case DMAP_BSOVERWORLD:
				showExplore = (module_arr[offs+M_FLAGS1] & FLAG_MMP_SHOW_EXPLORED_ROOMS_BSOW);
				break;
			case DMAP_DUNGEON:
				showExplore = (module_arr[offs+M_FLAGS1] & FLAG_MMP_SHOW_EXPLORED_ROOMS_DUNGEON);
				break;
			case DMAP_CAVE:
				showExplore = (module_arr[offs+M_FLAGS1] & FLAG_MMP_SHOW_EXPLORED_ROOMS_INTERIOR);
				break;
		}
		if(ow == DMAP_OVERWORLD)
		{
			for(int q = 0; q < 128; ++q)
			{
				int x = orig_x + 9 + (4*(q%0x010));
				int y = orig_y + 8 + (4*Div(q, 0x010));
				int c = 0;
				if(hasCompass && q == dm->Compass &&
				   (blinkon || (deadCompass && !(module_arr[offs+M_FLAGS1] & FLAG_MMP_COMPASS_BLINK_DOESNT_STOP))))
				{
					c = deadCompass ? module_arr[offs+P5] : module_arr[offs+P4];
				}
				else if(q == curscr)
					c = module_arr[offs+P1];
				else if(showExplore)
				{
					mapdata m = Game->LoadMapData(Game->GetCurMap(),q);
					if(m->State[ST_VISITED])
					{
						c = module_arr[offs+P2];
					}
				}
				
				if(c) bit->Rectangle(module_arr[offs+M_LAYER], x, y, x+2, y+2, module_arr[offs+P1], 1, 0, 0, 0, true, OP_OPAQUE);
			}
		}
		else
		{
			int dmoffs = Game->DMapOffset[Game->GetCurDMap()];
			bool largeMarkers = module_arr[offs+M_FLAGS1] & FLAG_MMP_LARGE_PLAYER_COMPASS_MARKERS;
			
			int lim = 8 - Max(dmoffs - 8, 0);
			int low_lim = Min(dmoffs, 0);
			
			for(int q = 0; q < 128; ++q)
			{			
				if(q % 0x10 >= lim)
					continue;
				if(q % 0x10 < low_lim)
					continue;
				
				int c = 0;
				int sm_c = 0;
				int x = orig_x + 8 + (8*(q%0x010));
				int y = orig_y + 8 + (4*Div(q, 0x010));
				
				if(hasCompass && q+dmoffs == dm->Compass &&
				   (blinkon || (deadCompass && !(module_arr[offs+M_FLAGS1] & FLAG_MMP_COMPASS_BLINK_DOESNT_STOP))))
				{
					c = deadCompass ? module_arr[offs+P5] : module_arr[offs+P4];
				}
				else if(q == curscr)
				{
					c = module_arr[offs+P1];
				}
				
				unless(largeMarkers)
				{
					if(c)
					{
						sm_c = c;
						c = 0;
					}
				}
				unless(c)
				{
					mapdata m = Game->LoadMapData(Game->GetCurMap(),q+dmoffs);
					if(showExplore && m->State[ST_VISITED])
					{
						c = module_arr[offs+P2];
					}
					else if(hasMap && dmapinfo::VisibleOnDungeonMap(q, true))
					{
						c = module_arr[offs+P3];
					}
				}
				if(c)
				{
					bit->Rectangle(module_arr[offs+M_LAYER], x, y, x+6, y+2, c, 1, 0, 0, 0, true, OP_OPAQUE);
				}
				if(sm_c)
				{
					bit->Rectangle(module_arr[offs+M_LAYER], x+2, y, x+4, y+2, sm_c, 1, 0, 0, 0, true, OP_OPAQUE);
				}
			}
		}
	} //end Minimap
